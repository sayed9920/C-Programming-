SAWPING BETWEEN TWO NUMBER:

#include <stdio.h>

#include <stdio.h>

int main() {
    int a = 4, b = 8, temp;

    printf("Before swapping: a = %d, b = %d\n", a, b);

    // Swapping using a temporary variable
    temp = a;
    a = b;
    b = temp;

    printf("After swapping: a = %d, b = %d\n", a, b);

    return 0;
}